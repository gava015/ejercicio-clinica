<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="337.0" prefWidth="319.0" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.clinica.controladores.RegistroPacienteControlador">


    <Label text="Registro de pacientes" textFill="#0f0f0f">
        <font>
            <Font size="20.0" />
        </font>
    </Label>
   <Label text="Cedula:">
      <effect>
         <InnerShadow />
      </effect>
   </Label>
   <TextField fx:id="txtCedula" prefHeight="31.0" prefWidth="392.0" />
   <Label prefHeight="33.0" prefWidth="125.0" text="Nombre:">
      <effect>
         <InnerShadow />
      </effect>
   </Label>
   <TextField fx:id="txtNombre" prefHeight="31.0" prefWidth="392.0" />
   <Label text="Telefono:">
      <effect>
         <InnerShadow />
      </effect>
   </Label>
   <TextField fx:id="txtTelefono" prefHeight="31.0" prefWidth="392.0" />
   <Label text="Correo:">
      <effect>
         <InnerShadow />
      </effect>
   </Label>
   <TextField fx:id="txtCorreo" prefHeight="31.0" prefWidth="392.0" />
   <Label text="Suscripcion:">
      <effect>
         <InnerShadow />
      </effect>
   </Label>
   <ComboBox fx:id="cbSuscripcion" prefWidth="150.0" />
   <Button mnemonicParsing="false" text="Registrar" onAction="#registrarPaciente" />
    
</VBox>
